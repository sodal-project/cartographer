<div class="p-8" style="max-width: 640px">
  {{> coreHeadline title="Module 1" }}

  <!-- List all of the passed data object's properties -->
  <div class="p-8 mb-6">
    {{> coreTable data=data moduleFolder="module1" deleteCommand="deleteConfig" }}
  </div>

  <!-- Form to post data to config db -->
  {{#> coreWindow }}
    {{#> coreForm moduleFolder="module1" command="writeConfig" }}
      {{> coreFormText label="Key" name="key" }}
      {{> coreFormText label="Value" name="value" }}
      {{> coreFormSubmit }}
    {{/coreForm}}
  {{/coreWindow}}
</div>